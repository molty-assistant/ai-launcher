{
  "expo": {
    "name": "AI Launcher",
    "slug": "ai-launcher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "ailauncher",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tommurton.ailauncher",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "chatgpt",
          "claude",
          "gemini",
          "mscopilot",
          "grok",
          "perplexity",
          "deepseek",
          "poe"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tommurton.ailauncher"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-haptics",
      [
        "react-native-reanimated/plugin"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
